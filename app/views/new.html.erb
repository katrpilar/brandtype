<div class="input-field col s12">
    <select>
      <option value="" disabled selected>Choose your position</option>
      <option value="1">Founder</option>
      <option value="2">Contractor</option>
      <option value="3">Press</option>
      <option value="4">Affiliate</option>
    </select>
    <label>Materialize Select</label>
  </div>
</body>

<div class="row">
    <=% form_with, html: { class = "col s12"} do |form| %>
      <div class="row">
        <div class="input-field col s6">
          <input id="brand_name" type="text" class="validate">
          <label for="brand_name">Brand Name</label>
        </div>
        <div class="input-field col s6">
          <input id="legal_name" type="text" class="validate">
          <label for="legal_name">Legal Business Name</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="business_url" type="text" class="validate">
          <label for="business_url">Company Website</label>
        </div>
      </div>
      <br>
      <p>
        <%= form.submit %>
      </p>  
    <% end %>
  </div>